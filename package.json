{
  "name": "lantern-fly-tracker",
  "version": "1.0.0",
  "description": "A web application for tracking and identifying dead lantern flies using computer vision",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && node src/server-db.js",
    "build": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "setup": "npm run install:all"
  },
  "keywords": [
    "lantern-fly",
    "computer-vision",
    "environmental-tracking",
    "mobile-web-app"
  ],
  "author": "HackCMU Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "axios": "^1.12.1",
    "form-data": "^4.0.4"
  }
}
